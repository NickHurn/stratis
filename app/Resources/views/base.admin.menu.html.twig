    <script>
        function hidePhone()
        {
            var request = $.ajax({
                url: "/devphone/clear",
                type: 'GET',
            });
            request.done(function (msg) {
                $('#devphone').hide();
            });
        }
        function checkPhone()
        {
            var request = $.ajax({
                url: "/devphone/check",
                type: 'GET',
            }).done(function (msg) {
                if(typeof msg !== 'undefined' && msg.length>2) {
                    $('#phonemsg').html(msg);
                    $('#devphone').show();
                    $('#phonebeep').get(0).play();
                }
            });
        }
        $(document).ready(function() {
            setTimeout(function(){ checkPhone(); }, 1500);
        });
    </script>
