{% extends 'base.html.twig' %}

{% block body %}

<div class='title checkabl'>
	<h2>Checkabl &raquo; Pre-Screen Questions &raquo; Software Development Manager</h2>
</div>
<div style='clear:both; margin:100px 0 30px; color:#888; font-size:16px'>
	Please complete the following questions as part of our pre-screening process for this position.
</div>

						
<div class="col-lg-12" style="margin-left:-21px">
			
	<div class="tab-content">
		<div class="settings">
			<article>
				<div class="col-xs-12 margin-vertically no-left">


				</div>
			</article>
			<article>

					<div class="col-md-12 no-left no-right" style="font-size:17px">
						<form method="post">
							<input type='hidden' name='qid' value='1'/>
							{{ form_html|raw }}
							<input type='submit' value='Save' class='btn btn-black' style="margin-left:7px">
						</form>
					</div>

			</article>
		</div>
	</div>
</div>

							
<script>
$(function() { 
	$('.datepicker').datepicker({
		dateFormat: 'dd-mm-yy',
		changeMonth: true,
		changeYear: true,
	});
})
</script>
{% endblock %}
