{% extends 'base.html.twig' %}

{% block body %}


<section>
    <div class="container-fluid">
        <div class="row">
            <div class="application-main-wrap">
                <div class="wrapper-innerbox">
                    <div class="page-title">
                        <div class="col-md-7">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="title checkabl">
                                        <h2>Directorship Checks</h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="col-md-7">
                        <div class="row">
<table>
							
										{% for emp in results %}
											<tr>
												<td>{{ emp.firstname }}
											</tr>
										{% else %}
                                            <li><small>If you have been known by other forenames and/or surnames, please add them here</small></li>
										{% endfor %}
</table>
						</div>
                </div>
            </div>
        </div>
    </div>
</section>
{% endblock %}
