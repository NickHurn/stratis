{% extends 'base.admin.menu.html.twig' %}

{% block body %}

<div class="col-lg-12">
	<div class="settings">

		<div class="col-xs-12 no-left">
			<div class="row col-xs-12 margin-vertically no-left">
				<h4>Testabl &raquo; Assign Test &raquo; {{form_name}}</h4>
			</div>
		</div>

		
		<div class="col-xs-12">
			<div class="row checklist">

				<form method='post'>
					<input type="hidden" name="tmp" value="1"/>
					{% for j in jobs %}
						<input type='checkbox' name='op{{j.id}}' id='op{{j.id}}' {% if j.jobId == j.id %} checked {% endif %} value="1"/><label for="op{{j.id}}">{{j.title}}</label> <br/>
					{% endfor %}
					<br/>
					<input type="submit" class="btn btn-black btn-small" value="Save"/>
				</form>
			</div>
		</div>	


	</div>
</div>

{% endblock %}
